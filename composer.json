{
    "name": "vinkla/shield",
    "description": "A HTTP basic auth middleware for Laravel",
    "license": "MIT",
    "keywords": [
        "auth",
        "basic",
        "http",
        "laravel",
        "middleware",
        "shield"
    ],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "homepage": "https://github.com/vinkla"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0",
        "symfony/http-kernel": "^6.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Vinkla\\Shield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Shield\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Shield\\ShieldServiceProvider"
            ]
        }
    }
}
